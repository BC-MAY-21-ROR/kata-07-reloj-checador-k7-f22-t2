<div class="table__create">
  <%= simple_form_for(@branch) do |f| %>
    <%= f.input :name, required: true %>
    <%= f.input :email, required: true %>
    <%= f.input :private_number, required: true %>
    <%= f.input :position, required: true %>
    <%= f.association :branch, prompt: "Select branch" %>
    <%= f.button :submit %>
  <% end %>
</div>